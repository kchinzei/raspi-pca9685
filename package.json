{
  "name": "raspi-pca9685-pwm",
  "version": "0.4.0",
  "description": "Hardware PWM by PCA9685 via I2C on the Raspberry Pi",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "lint",
    "build": "tsc",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml}\"",
    "lint:type": "tsc --noEmit",
    "test": "run-s lint test:*",
    "test:unit": "jest",
    "tslint": "tslint 'src/**/*.ts'"
  },
  "keywords": [
    "nodebots",
    "LED",
    "raspberry pi"
  ],
  "author": "Kiyo Chinzei",
  "license": "MIT",
  "contributors": [
    {
      "name": "Kiyo Chinzei",
      "email": "kchinzei@gmail.com"
    }
  ],
  "bundleDependencies": false,
  "config": {
    "unsafe-perm": true
  },
  "cpu": [
    "arm",
    "arm64"
  ],
  "dependencies": {
    "raspi-i2c": "^6.2.4",
    "raspi-board": "^7.3.1",
    "raspi-peripheral": "^3.0.4",
    "raspi-soft-pwm": "^6.0.2"
  },
  "deprecated": false,
  "devDependencies": {
    "@types/i2c-bus": "^5.1.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/raspi-board": "^5.2.0",
    "@types/raspi-i2c": "^6.2.4",
    "@types/raspi-peripheral": "^2.0.0",
    "@types/raspi-soft-pwm": "^6.0.2",
    "jest": "^25.3.0",
    "ts-jest": "^25.3.1",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "eslint": "^6.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4"
  },
  "engines": {
    "node": ">= 6.4.0"
  },
  "homepage": "https://github.com/kchinzei/raspi-pca9685-pwm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kchinzei/raspi-pca9685-pwm.git"
  },
  "types": "./declarations/index.d.ts"
}
